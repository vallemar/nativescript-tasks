{
    "compilerOptions": {
        "strict": true,
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "lib": [
          "esnext",
          "WebWorker"
        ],
        "sourceMap": true,
        "noEmitHelpers": true,
        "importHelpers": true,
        "baseUrl": ".",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
    },
    "include": [
        "."
      ],
      
  "exclude": [
    "node_modules"
  ]
}